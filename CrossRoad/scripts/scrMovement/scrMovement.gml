//cgvx+(cgvw*((randomNum[i]+1)/10))
switch (argument[0]) {
	case 0: return cgvx+(cgvw*.1); break;
	case 1: return cgvx+(cgvw*.23); break;
	case 2: return cgvx+(cgvw*.36); break;
	case 3: return cgvx+(cgvw*.5); break;
	case 4: return cgvx+(cgvw*.63); break;
	case 5: return cgvx+(cgvw*.76); break;
	case 6: return cgvx+(cgvw*.9); break;
}